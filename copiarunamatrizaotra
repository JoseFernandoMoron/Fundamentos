#include "stdafx.h"
#include "conio.h"
#include <iostream>
using namespace std;

int main(){
	int MATRIZ[100][100], f,c, MATRIZ2[100][100];
	cout<<"ingrese el numero de filas"<<endl;
	cin>>f;
	cout<<"ingrese el numero de columnas"<<endl;
	cin>>c;
	for(int i=0;i<f;i++){
		for(int j=0;j<c;j++){
			cout<<"ingrese el digito: ["<<i<<"]["<<j<<"]";
			cin>>MATRIZ[i][j];}}
	for(int i=0;i<f;i++){
		for(int j=0;j<c;j++){
			MATRIZ2[i][j]= MATRIZ[i][j];}}
	cout<<"la matriz 2 es:"<<endl;
	for(int i=0;i<f;i++){
		for(int j=0;j<c;j++){
			cout<<MATRIZ2[i][j];
		}
		cout<<"\n";

	}	


	getch();
	return 0;
}
