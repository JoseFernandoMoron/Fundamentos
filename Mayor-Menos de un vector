#include "stdafx.h"
#include "conio.h"
#include <iostream>
#define MAX 100

using namespace std;
void main(){
	int V[MAX];
	int n;
	int cnt=0;
	int cnt2=0;
	int cnt3=0;
	int may=0;
	int men=999999;
	cout<<"ingrese n"<<endl;
	cin>>n;
	int i=0;
	for(i=0;i<n;i++){
		cin>>V[i];}
	for(i=0;i<n;i++){
		if(V[i]>may){
			may=V[i];}
		else{
			if(V[i]<men){
				men=V[i];}}}
	cout<<" el mayor es: "<<may;
        cout<<"el menor es: "<<men;
	getch();}
