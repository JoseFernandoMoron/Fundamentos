#include "stdafx.h"
#include "conio.h"
#include <iostream>
#define MAX 100

using namespace std;
void main(){
	int V[MAX];
	int n,x;
	cout<<"ingrese n"<<endl;
	cin>>n;
	int i=0;
	int cnt=0;
	for(i=0;i<n;i++){
		cin>>V[i];}
	for(i=0;i<n;i++){
		cnt+=V[i];}
	x=cnt/n;
	if(x>n){
		cout<<"el promedio es mayor al tamano"<<endl;}
	else{
		cout<<"no es mayor que el tamano"<<endl;}
getch();}
